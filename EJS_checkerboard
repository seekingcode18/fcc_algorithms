function printLine(line) {
  line % 2 === 0 ? str = ' ' : str = '#';
  for (let i = 1; i < 8; i++) {
      if (str[str.length-1] === '#') {
          str += ' ';
      } else if (str[str.length-1] === ' ') {
          str+= '#';
      }
  }
  str += '!';
  console.log(str)
}

printLine(1)
printLine(2)
